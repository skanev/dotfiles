#!/usr/bin/env zsh

current_program=$(~/.scripts/tmux/current-app)

if [[ $current_program == "vim" ]]; then
  tmux send-keys M-v
elif [[ -n $DOTFILES_WSL ]]; then
  tmux load-buffer =(powershell.exe -noprofile -command Get-Clipboard) \; paste-buffer
else
  tmux load-buffer =(pbpaste) \; paste-buffer
fi
